{
	"name": "meteofrance_api",
	"version": "1.1.4",
	"description": "A simple API to get weather data from ®MétéoFrance",
	"scripts": {
		"test": "nodemon test.js --watch test.js --watch dist",
		"publish": "npm run build && npm publish --access public",
		"build": "tsc",
		"build_auto": "nodemon --watch src --exec \"npm run build\""
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"keywords": [
		"wheather",
		"meteo",
		"france",
		"meteofrance"
	],
	"author": "Alexis06030631",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git@github.com:Alexis06030631/meteofrance_api.git",
		"directory": "src",
		"branch": "main"
	},
	"bugs": {
		"url": "https://github.com/Alexis06030631/meteofrance_api/issues"
	},
	"homepage": "https://github.com/Alexis06030631/meteofrance_api/",
	"dependencies": {
		"axios": "^1.4.0"
	},
	"nodemonConfig": {
		"ignore": [
			"*.json"
		],
		"ext": "js,json,ts"
	},
	"devDependencies": {
		"@types/node": "^20.1.0",
		"nodemon": "^2.0.22",
		"typescript": "^5.0.4"
	},
	"release": {
		"branches": [
			"main"
		]
	}
}
